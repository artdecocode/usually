{
  "name": "usually",
  "version": "1.0.0",
  "description": "Print usage of a Node.js command-line application.",
  "main": "build",
  "scripts": {
    "t": "zoroaster -b",
    "test": "yarn t test/spec",
    "test-build": "BABEL_ENV=test-build yarn t test/spec",
    "test-all": "yarn-s test test-build",
    "test-watch": "yarn test --watch",
    "lint": "eslint .",
    "e": "node example",
    "example/": "yarn e example/example.js",
    "build": "babel src --out-dir build --source-maps"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/usually.git"
  },
  "keywords": [
    "usually"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/usually/issues"
  },
  "homepage": "https://github.com/artdecocode/usually#readme",
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.49",
    "@babel/core": "7.0.0-beta.49",
    "@babel/plugin-syntax-object-rest-spread": "7.0.0-beta.49",
    "@babel/plugin-transform-modules-commonjs": "7.0.0-beta.49",
    "@babel/register": "7.0.0-beta.49",
    "babel-plugin-transform-rename-import": "2.2.0",
    "eslint": "4.19.1",
    "eslint-config-artdeco": "1.0.0",
    "snapshot-context": "2.0.1",
    "yarn-s": "1.1.0",
    "zoroaster": "2.1.0"
  }
}
